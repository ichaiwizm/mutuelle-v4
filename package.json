{
  "name": "mutuelle",
  "version": "0.1.0-beta.1",
  "productName": "Mutuelle",
  "description": "Automatisation des adhésions mutuelle",
  "author": "France Épargne",
  "license": "UNLICENSED",
  "private": true,
  "main": "out/main/index.js",
  "dependencies": {
    "@tailwindcss/vite": "^4.1.17",
    "better-sqlite3": "^12.4.1",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.44.7",
    "electron-updater": "^6.6.2",
    "googleapis": "^142.0.0",
    "lucide-react": "^0.555.0",
    "playwright-extra": "^4.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.9.6",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.17",
    "zod": "^4.1.12"
  },
  "packageManager": "pnpm@10.21.0+sha512.da3337267e400fdd3d479a6c68079ac6db01d8ca4f67572083e722775a796788a7a9956613749e000fac20d424b594f7a791a5f4e2e13581c5ef947f26968a40",
  "devDependencies": {
    "@electron/rebuild": "^4.0.1",
    "@playwright/test": "^1.56.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^24.10.0",
    "@types/prompts": "^2.4.9",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/parser": "^8.46.4",
    "@vitejs/plugin-react": "^5.1.0",
    "@vitest/coverage-v8": "^4.0.8",
    "cross-env": "^10.1.0",
    "dotenv": "^16.4.5",
    "drizzle-kit": "^0.31.6",
    "electron": "^28.3.3",
    "electron-builder": "^26.0.12",
    "electron-vite": "^4.0.1",
    "eslint": "^9.39.1",
    "jsdom": "^27.1.0",
    "playwright": "^1.56.1",
    "prompts": "^2.4.2",
    "start-server-and-test": "^2.1.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vite": "^7.2.2",
    "vitest": "^4.0.8"
  },
  "vitest": {
    "setupFiles": [
      "dotenv/config"
    ]
  },
  "scripts": {
    "platform:switch": "bash scripts/switch-platform.sh",
    "dev": "electron-vite dev",
    "dev:3s": "node -e \"const {spawn}=require('child_process');const p=spawn('pnpm',['dev'],{detached:true,stdio:'ignore'});p.unref();setTimeout(()=>{console.log('Electron dev lancé (pid '+p.pid+').');},3000);\"",
    "build": "eslint . && electron-vite build",
    "test": "vitest run --pool=forks",
    "test:ui": "vitest",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "lead-test": "tsx scripts/select-lead.ts",
    "gmail:auth": "tsx scripts/gmail-auth.ts",
    "seed:products": "tsx scripts/seed-products.ts",
    "rebuild": "electron-rebuild -f -w better-sqlite3",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "dist": "electron-vite build && electron-builder",
    "dist:mac": "electron-vite build && electron-builder --mac",
    "dist:win": "electron-vite build && electron-builder --win"
  },
  "build": {
    "appId": "com.france-epargne.mutuelle",
    "productName": "Mutuelle",
    "directories": {
      "output": "release"
    },
    "files": [
      "out/**/*",
      "drizzle/**/*",
      "node_modules/**/*",
      "!node_modules/**/*.map",
      "!node_modules/**/test/**",
      "!node_modules/**/docs/**"
    ],
    "mac": {
      "target": ["dmg"],
      "category": "public.app-category.business",
      "icon": "build/icon.icns"
    },
    "win": {
      "target": ["nsis"],
      "icon": "build/icon.ico"
    },
    "extraResources": [
      {
        "from": "drizzle",
        "to": "drizzle"
      },
      {
        "from": ".playwright-browsers",
        "to": "playwright-browsers"
      }
    ],
    "publish": {
      "provider": "github",
      "owner": "ichaiwizm",
      "repo": "mutuelle-v4"
    }
  }
}
