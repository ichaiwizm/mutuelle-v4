{
  "name": "mutuelle-test-forms",
  "version": "1.0.0",
  "description": "POC test system for form automation with Playwright",
  "type": "module",
  "scripts": {
    "dev": "tsx server/index.ts",
    "server": "tsx server/index.ts",
    "server:dev": "tsx watch server/index.ts",
    "test": "playwright test tests/form-automation.spec.ts",
    "test:ui": "playwright test tests/form-automation.spec.ts --ui",
    "test:headed": "playwright test tests/form-automation.spec.ts --headed",
    "test:debug": "playwright test tests/form-automation.spec.ts --debug",
    "test:premium": "playwright test tests/premium/premium-automation.spec.ts",
    "test:premium:ui": "playwright test tests/premium/premium-automation.spec.ts --ui",
    "test:premium:headed": "playwright test tests/premium/premium-automation.spec.ts --headed",
    "test:premium:debug": "playwright test tests/premium/premium-automation.spec.ts --debug",
    "test:alptis": "playwright test tests/alptis-sante-select/login.spec.ts",
    "test:alptis:ui": "playwright test tests/alptis-sante-select/login.spec.ts --ui",
    "test:alptis:headed": "playwright test tests/alptis-sante-select/login.spec.ts --headed",
    "test:alptis:debug": "playwright test tests/alptis-sante-select/login.spec.ts --debug",
    "ui:alptis": "playwright test tests/alptis-sante-select/login.spec.ts --ui"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/node": "^20.10.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
