{
  "name": "mutuelle-test-forms",
  "version": "1.0.0",
  "description": "POC test system for form automation with Playwright",
  "type": "module",
  "scripts": {
    "server": "tsx server/index.ts",
    "server:dev": "tsx watch server/index.ts",
    "test": "playwright test tests/form-automation.spec.ts",
    "test:ui": "playwright test tests/form-automation.spec.ts --ui",
    "test:headed": "playwright test tests/form-automation.spec.ts --headed",
    "test:debug": "playwright test tests/form-automation.spec.ts --debug"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/node": "^20.10.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
